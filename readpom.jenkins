pipeline {
    agent any
    stages {
        stage ('Prepare') {
            steps {
			
                script {
            		def pom = readMavenPom file: 'pom.xml'
			def (releaseVersion, shacommit) = pom.version.split('-')
			env.releaseVersion=releaseVersion
                }
            }
        }
	
	stage ('test') {
            steps {
			
               sh "echo ${env.releaseVersion}"
            }
        }
    }
}
